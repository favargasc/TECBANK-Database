<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="-2dp"
    tools:layout_editor_absoluteY="-44dp">

    <TextView
        android:id="@+id/lblNombreApellido"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:text="Nombre Apellido"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="84dp" />

    <ScrollView
        android:layout_width="345dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="525dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <TextView
        android:id="@+id/lblSobresAhorro"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:text="Sobres De Ahorro"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="481dp" />

    <TextView
        android:id="@+id/lblMostrarNombeAp"
        android:layout_width="343dp"
        android:layout_height="31dp"
        android:text="Mostrar Nombre y apellido"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="123dp" />

    <TextView
        android:id="@+id/lblCorreoElectr"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:text="Correo electronico"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:id="@+id/lblMostrarCorreo"
        android:layout_width="343dp"
        android:layout_height="31dp"
        android:text="Mostrar Correo Electronico"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="209dp" />

    <TextView
        android:id="@+id/lblNombreUsuario"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:text="Nombre Usuario"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/lblMostrarNomUs"
        android:layout_width="343dp"
        android:layout_height="31dp"
        android:text="Mostrar Nombre Usuario"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="289dp" />

    <TextView
        android:id="@+id/lblCuentas"
        android:layout_width="200dp"
        android:layout_height="32dp"
        android:text="Cuentas"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/lblInfoUs"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:text="Informacion de Usuario"
        android:textAllCaps="true"
        android:textDirection="firstStrongRtl"
        android:textIsSelectable="false"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="28dp" />

    <ScrollView
        android:id="@+id/spnCuentas"
        android:layout_width="338dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="372dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <Button
        android:id="@+id/btnHistorial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historial"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="655dp" />
</androidx.constraintlayout.widget.ConstraintLayout>